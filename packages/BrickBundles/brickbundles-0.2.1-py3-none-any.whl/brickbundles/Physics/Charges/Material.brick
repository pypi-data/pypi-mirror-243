#"""
#Base type for Material
#"""
const Material:
    .doc: """
    The Material model defines the properties of a geometry.

    """
    density is Real: 1000.0
    youngs_modulus is Real: 100000000.0
    relaxation_time is Real: 0.05
    damping_capacity is Real: this.youngs_modulus * this.relaxation_time

    elastic_limit is Real: Math.INF
    failure_limit is Real: Math.INF
    poissons_ratio is Real: 0.5
