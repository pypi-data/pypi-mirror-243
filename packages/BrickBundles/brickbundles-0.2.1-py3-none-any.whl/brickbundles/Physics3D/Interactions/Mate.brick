Mate is Interaction:
    charges becomes Physics3D.Charges.MateConnector[]
    stiffness is MateStiffness
    damping is MateDamping:
        along_main: stiffness.along_main / 20.0
        along_normal: stiffness.along_normal / 20.0
        along_cross: stiffness.along_cross / 20.0
        around_main: stiffness.around_main / 20.0
        around_normal: stiffness.around_normal / 20.0
        around_cross: stiffness.around_cross / 20.0
