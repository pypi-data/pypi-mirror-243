{
	"python.testing.pytestArgs": [],
	"python.testing.unittestEnabled": false,
	"python.testing.pytestEnabled": true,
	"python.formatting.provider": "black",
	"editor.formatOnSave": true,
	"search.exclude": {
		"/home/vscode/.local/lib/python3.10/site-packages/bencher/**": true
	},
	"files.exclude": {
		"/home/vscode/.local/lib/python3.10/site-packages/bencher/**": true
	},
	"files.watcherExclude": {
		"**/cachedir/**": true,
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter"
	},
	"files.exclude": {
		"**/__pycache__/**": true
	},
	"python.analysis.autoImportCompletions": false,
	"workbench.colorCustomizations": {
		"activityBar.background": "#113507",
		"titleBar.activeBackground": "#19440d",
		"titleBar.activeForeground": "#F4FDF1"
	} //vscode gets it wrong more than right and mostly gets in the way
}