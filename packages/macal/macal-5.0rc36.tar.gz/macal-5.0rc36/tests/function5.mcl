// Back to Fibonacci numbers again

include system;

fib => (n) {
    if n == 1 or n == 2 {
        return 1;
    }
    return fib(n-1) + fib(n-2);
}

  print("Fibonacci for  1: ", fib(1));
  print("Fibonacci for  2: ", fib(2));
Console("Fibonacci for  3: ", fib(3));
